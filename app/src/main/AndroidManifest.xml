<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.CAMERA"/>



    <application
        android:name="com.example.umte_projekt.App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.UMTEprojekt"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name="com.example.umte_projekt.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.UMTEprojekt">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.umte_projekt.ui.basic.form.DepotHomeAcitvity"
            android:exported="false"
            android:theme="@style/Theme.UMTEprojekt"/>
        <activity
            android:name="com.example.umte_projekt.ui.readerQR.BarcodeAnalyserActivity"
            android:exported="false"
            android:theme="@style/Theme.UMTEprojekt"
            tools:ignore="MissingClass" />

        <activity
            android:name="com.example.umte_projekt.ui.basic.form.FormLoginActivity"
            android:exported="false"
            android:theme="@style/Theme.UMTEprojekt"/>
        <activity
            android:name="com.example.umte_projekt.ui.basic.form.FormDepotActivity"
            android:exported="false"
            android:theme="@style/Theme.UMTEprojekt" />

        <activity
            android:name="com.example.umte_projekt.ui.basic.lazylist.PartLazyListActivity"
            android:exported="false"
            android:theme="@style/Theme.UMTEprojekt" />

        <activity
            android:name="com.example.umte_projekt.ui.basic.form.AnotherUserActivity"
            android:exported="false"
            android:theme="@style/Theme.UMTEprojekt" />

    </application>

</manifest>